<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Subsystem">
	<!-- 保存对象 -->
	<insert id="createSubsystem" parameterType="com.gdyunst.ystadmin.framework.domain.entity.admin.SubsystemEntity">
		<![CDATA[
            INSERT INTO admin_subsystem
                (id,code,text,url,indexOrder,creater,operator,operateDate,note)
            VALUES 
                (#{id},#{code},#{text},#{url},#{indexOrder},#{creater},#{operator},#{operateDate},#{note})
        ]]>
	</insert>

	<!-- 更新对象 -->
	<update id="updateSubsystem" parameterType="com.gdyunst.ystadmin.framework.domain.entity.admin.SubsystemEntity">
        <![CDATA[
            UPDATE admin_subsystem SET id=id
        ]]>
		<if test="note!=null and note!=''">,note=#{note}</if>
		<if test="operateDate!=null ">,operateDate=#{operateDate}</if>
		<if test="operator!=null and operator!=''">,operator=#{operator}</if>
		<if test="createDate!=null">,createDate=#{createDate}</if>
		<if test="creater!=null and creater!=''">,creater=#{creater}</if>
		<if test="indexOrder!=null ">,indexOrder=#{indexOrder}</if>
		<if test="url!=null and url!=''">,url=#{url}</if>
		<if test="text!=null and text!=''">,text=#{text}</if>
		<if test="code!=null and code!=''">,code=#{code}</if>
		<if test="id!=null and id!=''">,id=#{id}</if>
		
        <![CDATA[
            WHERE id=#{id}
        ]]>
	</update>

	<!-- 查找单个对象 -->
	<select id="loadSubsystem" resultType="com.gdyunst.ystadmin.framework.domain.entity.admin.SubsystemEntity"
		parameterType="com.gdyunst.ystadmin.framework.domain.entity.admin.SubsystemEntity">
		SELECT * FROM admin_subsystem WHERE id=#{id}
	</select>

	<!-- 查询数据列表 -->
	<select id="listSubsystem" resultType="com.gdyunst.ystadmin.framework.domain.entity.admin.SubsystemEntity"
		parameterType="com.gdyunst.ystadmin.framework.domain.entity.admin.SubsystemEntity">
        <![CDATA[
	  		SELECT * FROM admin_subsystem WHERE 1=1
	  	]]>
		<if test="note!=null and note!=''">AND note=#{note}</if>
		<if test="operateDate!=null'">AND operateDate=#{operateDate}</if>
		<if test="operator!=null and operator!=''">AND operator=#{operator}</if>
		<if test="createDate!=null">AND createDate=#{createDate}</if>
		<if test="creater!=null and creater!=''">AND creater=#{creater}</if>
		<if test="indexOrder!=null'">AND indexOrder=#{indexOrder}</if>
		<if test="url!=null and url!=''">AND url=#{url}</if>
		<if test="text!=null and text!=''">AND text=#{text}</if>
		<if test="code!=null and code!=''">AND code=#{code}</if>
		<if test="id!=null and id!=''">AND id=#{id}</if>
		
		<![CDATA[
			ORDER BY indexOrder DESC
		]]>
	</select>

	<!-- 删除单个对象 -->
	<delete id="removeSubsystem" parameterType="com.gdyunst.ystadmin.framework.domain.entity.admin.SubsystemEntity">
		UPDATE
		admin_subsystem SET delStatus=1 WHERE id=#{id}
	</delete>

	<!-- 批量插入 -->
	<insert id="insertBatchSubsystem" parameterType="java.util.List">
		<![CDATA[
            INSERT INTO admin_subsystem
                (id,code,text,url,indexOrder,creater,operator,operateDate,note)
            VALUES
        ]]>
		<foreach collection="list" item="item" index="index"
			separator=",">
			(#{item.id},#{item.code},#{item.text},#{item.url},#{item.indexOrder},#{item.creater},#{item.operator},#{item.operateDate},#{item.note})
		</foreach>
	</insert>


</mapper>
